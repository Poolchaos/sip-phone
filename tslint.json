{
  "rules": {
    "class-name": true,
    "only-arrow-functions": false,
    "no-magic-numbers": false,
    "typedef": [
      true,
      "call-signature",
      "property-declaration",
      "parameter"
    ],
    "curly": true,
    "no-console": [
      true,
      "log",
      "debug",
      "error",
      "warn"
    ],
    "triple-equals": true
  }
}